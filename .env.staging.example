# ===========================================
# AutoBuild Orchestrator - STAGING Environment
# ===========================================

NODE_ENV=staging

# Supabase Staging Project
DATABASE_URL=postgresql://postgres.[STAGING_PROJECT_REF]:[PASSWORD]@aws-0-[REGION].pooler.supabase.com:6543/postgres

# GitHub OAuth App (Staging)
GITHUB_CLIENT_ID=staging_github_client_id
GITHUB_CLIENT_SECRET=staging_github_client_secret

# GitHub Access Token
GITHUB_ACCESS_TOKEN=ghp_your_staging_access_token

# Security (use different values than production!)
SESSION_SECRET=staging_session_secret_32_chars_min
ENCRYPTION_KEY=staging_encryption_key_32_chars_

# Staging URL
APP_URL=https://staging.your-app.com
