NODE_ENV=production

# Supabase Production Project
DATABASE_URL=postgresql://postgres.[PROD_PROJECT_REF]:[PASSWORD]@aws-0-[REGION].pooler.supabase.com:6543/postgres

# GitHub OAuth App (Production)
GITHUB_CLIENT_ID=prod_github_client_id
GITHUB_CLIENT_SECRET=prod_github_client_secret

# GitHub Access Token
GITHUB_ACCESS_TOKEN=ghp_your_production_access_token

# Security (use strong, unique values!)
SESSION_SECRET=production_session_secret_generate_with_openssl
ENCRYPTION_KEY=production_encryption_key_generate_with_openssl

# Production URL
APP_URL=https://your-app.com
